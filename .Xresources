! Font ---------------------------------------

!URxvt.font           : xft:RobotoMono Nerd Font Mono:style=Medium:size=12, xft:Iosevka Nerd Font:style=Medium:size=13, xft:Font Awesome 5 Free Solid:style=Solid, xft:Material Design Icons:style=Regular, xft:feather:style=Regular
URxvt.font           : xft:SFMono Nerd Font:style=Medium:size=12, xft:RobotoMono Nerd Font Mono:style=Medium:size=12, xft:FontAwesome:style=Regular, xft:Material\-Design\-Iconic\-Font:style=Design-Iconic-Font, xft:feather:style=Regular, xft:icomoon:style=Regular, xft:octicons:style=Medium
URxvt.boldFont       : xft:SFMono Nerd Font:style=Bold:size=12
URxvt.italicFont     : xft:SFMono Nerd Font:style=Italic:size=12
URxvt.boldItalicFont : xft:SFMono Nerd Font:style=BoldItalic:size=12

Xft.dpi           : 96
Xft.antialias     : true
Xft.rgba          : rgb
Xft.autohint      : true
Xft.lcdfilter     : lcddefault
Xft.hinting       : true
Xft.hintstyle     : hintfull
URxvt.letterSpace : 0

! Color -------------------------------------

! Purify
#define black0      #282C34
#define black1      #1D2637
#define black2      #1D2637
#define red0        #FF6059
#define red1        #FF6059
#define green0      #5FFF98
#define green1      #5FFF87
#define yellow0     #FFFF87
#define yellow1     #FFFF87
#define blue0       #5FAFFF
#define blue1       #5FAFFF
#define magenta0    #AF87FF
#define magenta1    #AF87FF
#define cyan0       #5FAFFF
#define cyan1       #5FAFFF
#define white0      #FFFFFF
#define white1      #FFFFFF

#define grey        #3E4452

*.color1 : red0
*.color2 : green0
*.color3 : yellow0
*.color4 : blue0
*.color5 : magenta0
*.color6 : cyan0

*.color9  : red1
*.color10 : green1
*.color11 : yellow1
*.color12 : blue1
*.color13 : magenta1
*.color14 : cyan1

*.foreground  : white0
*.background  : black0
*.cursorColor : white0
*.borderColor : black0

*.color0 : black2
*.color7 : white1

*.color8  : black2
*.color15 : white1

!*.foreground  : black0
!*.background  : white0
!*.cursorColor : black0
!*.borderColor : white0

!*.color0 : white1
!*.color7 : black2

!*.color8  : white1
!*.color15 : black2

! Settings ----------------------------------

URxvt.iconFile             : /home/josh/.icons/Custom/terminal.svg
!URxvt.backgroundPixmap     : /home/josh/.urxvt/backgrounds/paper.png
URxvt.windowName           : urxvt
URxvt.windowTitle          : urxvt
URxvt.termName             : xterm-256color
URxvt.borderLess           : false
!URxvt.internalBorder       : 20
!URxvt.externalBorder       : 0
URxvt.geometry             : 90x27
URxvt.skipBuiltinGlyphs    : false
URxvt.scrollTtyKeypress    : true
URxvt.scrollTtyOutput      : false
URxvt.scrollWithBuffer     : true
URxvt.scrollstyle          : rxvt
URxvt.scrollBar            : floating
URxvt.urgentOnBell         : true
URxvt.visualBell           : true
URxvt.saveLines            : 100000
URxvt.mouseWheelScrollPage : false
URxvt.cursorBlink          : true
URxvt.cursorBlock          : true
!URxvt.inheritPixmap        : false
URxvt.transparent          : false
!URxvt.shading              : 0
URxvt.depth                : 32
URxvt.secondaryScreen      : 1
URxvt.secondaryScroll      : 0
URxvt.iso14755             : false
URxvt.iso14755_52          : false
Xcursor.theme              : Bibata-Modern-Ice
Xcursor.size               : 24

! Extensions --------------------------------

URxvt.perl-ext-common: clipboard,font-size
URxvt.perl-lib       : /usr/lib/urxvt/perl/

! Clipboard [Ctrl+Shift+C/V]
URxvt.clipboard.copycmd  : xclip -i -selection clipboard
URxvt.clipboard.pastecmd : xclip -o -selection clipboard
URxvt.keysym.C-S-C       : perl:clipboard:copy
URxvt.keysym.C-S-V       : perl:clipboard:paste

! Font-size
!URxvt.keysym.C-Up     : font-size:increase
!URxvt.keysym.C-Down   : font-size:decrease
!URxvt.keysym.C-S-Up   : font-size:incglobal
!URxvt.keysym.C-S-Down : font-size:decglobal
!URxvt.keysym.C-equal  : font-size:reset
!URxvt.keysym.C-slash  : font-size:show
